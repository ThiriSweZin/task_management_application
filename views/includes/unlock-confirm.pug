div.modal.fade#dialogUnlockConfirm(tabindex='-1', role='dialog', aria-labelledby='unlockConfirm')
  div.modal-dialog.modal-sm(role='document')
    div.modal-content
      div.modal-header
        h4.modal-title Unlock
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      div.modal-body
        p Are you sure to unlock?
      div.modal-footer
        button.btn.btn-primary#dialogAccept(type='button', data-dismiss='modal') OK
        button.btn.btn-secondary#dialogCancel(type='button', data-dismiss='modal') Cancel

div.alert.alert-success.alert-dismissible#alertUnlockSuccess(role='alert', style="display:none;")
  strong Success 
  span#delSuccessMsg Unlock Successful.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;

div.alert.alert-danger.alert-dismissible#alertUnlockError(role='alert', style="display:none;")
  strong Error 
  span#delErrorMsg Can not unlock data.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;