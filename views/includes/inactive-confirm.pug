div.modal.fade#dialogInactiveConfirm(tabindex='-1', role='dialog', aria-labelledby='inactiveConfirm')
    div.modal-dialog.modal-sm(role='document')
        div.modal-content
            div.modal-header
                h4.modal-title InActive
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') &times;
            div.modal-body
                p Are you sure to inactive?
            div.modal-footer
                button.btn.btn-primary#dialogAccept(type='button', data-dismiss='modal') Ok
                button.btn.btn-secondary#dialogCancel(type='button', data-dismiss='modal') Cancel

div.alert.alert-success.alert-dismissible#alertInActiveSuccess(role='alert', style="display:none;")
    strong Success 
    span#delSuccessMsg InActive Successful.
    button.close(type='button', data-hide='alert', aria-label='Close')
        span(aria-hidden='true') &times;

div.alert.alert-danger.alert-dismissible#alertInActiveError(role='alert', style="display:none;")
    strong Error 
    span#delErrorMsg Can not inactive customer.
    button.close(type='button', data-hide='alert', aria-label='Close')
        span(aria-hidden='true') &times;