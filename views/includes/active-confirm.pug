div.modal.fade#dialogActiveConfirm(tabindex='-1', role='dialog', aria-labelledby='activeConfirm')
    div.modal-dialog.modal-sm(role='document')
        div.modal-content
            div.modal-header
                h4.modal-title Active
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') &times;
            div.modal-body
                p Are you sure to active?
            div.modal-footer
                button.btn.btn-primary#dialogAccept(type='button', data-dismiss='modal') Ok
                button.btn.btn-secondary#dialogCancel(type='button', data-dismiss='modal') Cancel

div.alert.alert-success.alert-dismissible#alertActiveSuccess(role='alert', style="display:none;")
    strong Success 
    span#delSuccessMsg Active Successful.
    button.close(type='button', data-hide='alert', aria-label='Close')
        span(aria-hidden='true') &times;

div.alert.alert-danger.alert-dismissible#alertActiveError(role='alert', style="display:none;")
    strong Error 
    span#delErrorMsg Can not active data.
    button.close(type='button', data-hide='alert', aria-label='Close')
        span(aria-hidden='true') &times;